config tinc-net librevpn
	option enabled 1

	list ConnectTo rasputin
	list ConnectTo medieval
	list ConnectTo diablo
	list ConnectTo ponape
	list ConnectTo gaspacho

	option Mode switch
	option Name gaspacho

config tinc-host salaalberdi
	option enabled 1

	option net librevpn
	option Subnet 192.168.9.253/32
